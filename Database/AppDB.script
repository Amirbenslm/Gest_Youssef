CREATE MEMORY TABLE PUBLIC.BILL(CODE VARCHAR(20) COLLATE SQL_TEXT_UCC NOT NULL PRIMARY KEY,DATE TIMESTAMP NOT NULL,DISCOUNT DOUBLE NOT NULL,CODE_CLIENT VARCHAR(30) COLLATE SQL_TEXT_UCC NOT NULL,CODE_DEPOT INTEGER NOT NULL,CHECK((PUBLIC.BILL.DISCOUNT>=0) AND (PUBLIC.BILL.DISCOUNT<=100)),CONSTRAINT FKBILLCLIENT FOREIGN KEY(CODE_CLIENT) REFERENCES PUBLIC.CLIENT(CODE) ON DELETE CASCADE ON UPDATE CASCADE,CONSTRAINT FKBILLDEPOT FOREIGN KEY(CODE_DEPOT) REFERENCES PUBLIC.DEPOT(CODE) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE MEMORY TABLE PUBLIC.BILLPRODUCTS(CODE_BILL VARCHAR(20) COLLATE SQL_TEXT_UCC NOT NULL,CODE_PRODUCT VARCHAR(20) COLLATE SQL_TEXT_UCC NOT NULL,QNT DOUBLE NOT NULL,PRICE DOUBLE NOT NULL,PRIMARY KEY(CODE_BILL,CODE_PRODUCT),CONSTRAINT FKBILLPRODUCT FOREIGN KEY(CODE_PRODUCT) REFERENCES PUBLIC.PRODUCT(CODE) ON DELETE CASCADE ON UPDATE CASCADE,CONSTRAINT FKBILLCODE FOREIGN KEY(CODE_BILL) REFERENCES PUBLIC.BILL(CODE) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE MEMORY TABLE PUBLIC.PAYMENT(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DATE TIMESTAMP NOT NULL,TYPE VARCHAR(5) COLLATE SQL_TEXT_UCC NOT NULL,AMOUNT DOUBLE NOT NULL,CODE_BILL VARCHAR(20) COLLATE SQL_TEXT_UCC NOT NULL,CHECK((PUBLIC.PAYMENT.TYPE) IN (('cash'),('check'))),CONSTRAINT FKPAYMENTBILLCODE FOREIGN KEY(CODE_BILL) REFERENCES PUBLIC.BILL(CODE) ON DELETE CASCADE ON UPDATE CASCADE)
