CREATE MEMORY TABLE PUBLIC.TRANSFERSTOCK(CODE INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DATE TIMESTAMP NOT NULL,TODEPOT INTEGER NOT NULL,FROMDEPOT INTEGER NOT NULL,CONSTRAINT FKTRANSFERSTOCKTODEPOT FOREIGN KEY(TODEPOT) REFERENCES PUBLIC.DEPOT(CODE) ON DELETE CASCADE ON UPDATE CASCADE,CONSTRAINT FKTRANSFERSTOCKFROMDEPOT FOREIGN KEY(FROMDEPOT) REFERENCES PUBLIC.DEPOT(CODE) ON DELETE CASCADE ON UPDATE CASCADE)
